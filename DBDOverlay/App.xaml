<Application x:Class="DBDOverlay.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DBDOverlay">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UI/Styles/Palette.xaml"/>
                <ResourceDictionary Source="UI/Styles/ButtonStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/ComboBoxStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/LabelStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/MenuTabStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/ScrollViewerStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/SliderStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/TextBlockStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/ToggleButtonStyle.xaml"/>
                <ResourceDictionary Source="UI/Styles/ToolTipStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
