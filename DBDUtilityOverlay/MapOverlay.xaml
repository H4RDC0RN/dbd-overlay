<Window x:Class="DBDUtilityOverlay.MapOverlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBDUtilityOverlay"
        mc:Ignorable="d"
        Title="MapOverlay" Height="220" Width="200"
        MouseLeftButtonDown="OverlayMouseDown"
        MouseLeftButtonUp="OverlayMouseUp"
        Cursor="Hand"
        WindowStyle="None"
        AllowsTransparency="True"
        ShowInTaskbar="False"
        Topmost="True">
    <Grid x:Name="MapOverlayGrid"/>
</Window>
